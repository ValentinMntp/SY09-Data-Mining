intervalle_confiance <- function(v){
m = mean(v)
s = sd(v)
n = length(v)
error <- qnorm(0.95)*s/sqrt(n)
left <- m-error
right <- m + error
intervalle <- c(left, m, right)
names(intervalle) <- c("left", "erreur", "right")
intervalle
}
